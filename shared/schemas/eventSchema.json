{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "BrowsingEvent",
  "type": "object",
  "properties": {
    "url": { "type": "string" },
    "eventType": { "type": "string" },
    "timestamp": { "type": "string", "format": "date-time" },
    "timeSpent": { "type": "number" },
    "mood": { "type": "string" },
    "element": { "type": "string" }
  },
  "required": ["url", "eventType", "timestamp", "timeSpent"]
}
